                <div class="aa-properties-item-content">
                  <div class="aa-properties-info">
                    <span><%=data[i].ptype%></span>
                  </div>
                  <div class="aa-properties-about">
                    <h3><a href="#"><%= data[i].pname%></a></h3>
                    <p><%=data[i].area%>,<%=data[i].city%></p>                      
                  </div>
                  <div class="aa-properties-detial">
                    <span class="aa-price">
                     Rs.<%= data[i].price%>
                    </span>
                    <a href="/property/propertyDetail/<%= data[i]._id%>" class="aa-secondary-btn">View Details</a>

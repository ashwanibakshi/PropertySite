  <!-- Advance Search -->
  <section id="aa-advance-search">
    <div class="container">
      <div class="aa-advance-search-area">
        <div class="form">
         <div class="aa-advance-search-top">
            <div class="row">
              <form action="/property/searchProperty" method="GET">
              <div class="col-md-4">
                <div class="aa-single-advance-search">
                  <select name="state">
                   <option value="">Select State</option>
                   <%for(var i=0;i< state.length;i++){%>
                   <option value="<%= state[i]%>"><%= state[i]%></option>
                   <%}%>
                  </select>
                </div>
              </div>
              <div class="col-md-2">
                <div class="aa-single-advance-search">
                  <select name="ptype" required>
                   <option value="" selected>Property Type</option>
                    <option value="flat">Flat</option>
                    <option value="land">Villa</option>
                    <option value="plot">PG</option>
                    <option value="hostel">Hostel</option>
                    <option value="commercial">Commercial</option>
                  </select>
                </div>
              </div>
              <div class="col-md-2">
                 <div class="aa-single-advance-search">
                  <select name="pfor" required>
                    <option value="" selected>Property For</option>
                    <option value="rent">Rent</option>
                    <option value="sale">Sale</option>
                    <option value="lease">Lease</option>
                  </select>
              </div>
              </div>
              <div class="col-md-2">
                 <div class="aa-single-advance-search">
                  <select name="price" required>
                    <option value="" selected>Price</option>
                    <option value="1000-10000">1000-10000</option>
                    <option value="10000-50000">10000-50000</option>
                    <option value="50000-100000">50000-100000</option>
                    <option value="above 100000">Above 100000</option>
                  </select>
                 </div>
              </div>
              <div class="col-md-2">
                <div class="aa-single-advance-search">
                  <input class="aa-search-btn" type="submit" value="Search">
                </div>
              </div>
              </form>
            </div>
          </div>

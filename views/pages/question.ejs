<%- include('../partial/header2')%>
 <section id="aa-signin">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-signin-area">
            <div class="aa-signin-form">
              <div class="aa-signin-form-title">
                <a class="aa-property-home" href="/property/Home">Property Home</a>
                <h4>Security Question</h4>
              </div>
              <%- include('../partial/messages')%>
              <form class="contactform" action="/user/question" method="POST">                                                 
                <div class="aa-single-field">
                  <label for="question">Question.&nbsp;&nbsp; <%=quest[0].question%> </label>
                </div>
                <div class="aa-single-field">
                    <label for="answer">Answer</label>
                    <input type="text" required="required" aria-required="true" name="answer">
                  </div>
                  <input type="hidden" name="email" value="<%= quest[0].email%>">
                <div class="aa-single-submit">
                  <input type="submit" value="Submit" class="aa-browse-btn" name="submit">  
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%- include('../partial/footer2')%>